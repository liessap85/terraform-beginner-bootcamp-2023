#!/usr/bin/env-bash

cd /workspace

#Add in this line to remove the file if it lready exists and reinstall

$FILENAME='awscliv2.zip'

rm  -f 'workspace/$FILENAME'
rm -rf 'workspace/aws'

curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
unzip awscliv2.zip
sudo ./aws/install

cd $PROJECT_ROOT
